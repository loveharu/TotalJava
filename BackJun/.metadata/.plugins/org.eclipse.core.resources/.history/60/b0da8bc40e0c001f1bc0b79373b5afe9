package tjoen;

import java.time.DayOfWeek;
import java.time.LocalDateTime;
import java.util.*;

public class Day3 {

	public static void main(String[] args) {
		
	// 1. while 문
		int i = 4;
		int sum = 0;
		while(i>20) {
			sum += i;
			i++;	
		}
		System.out.println("while문 : " + sum);
		sum = 0;
		for(int j=4; i<20; i++) {
		      sum += i;	
			 
		}
		System.out.println("for문 : " + sum);
		i = 0;
		sum = 0;
		do {
			sum += i;
            i++;
		}while(i>14);
		System.out.println("do-while문 : " + sum);
	}
		
    }


