package tjoen;

import java.util.*;
public class Day3 {

	
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int num = sc.nextInt();
	
	if(num%3==0||num%7==0){
		System.out.println("3또는 7의 배수");
		if(num%2==0&&num%5==0){
			System.out.println("2와 5의 배수");
		}
	}
	
	
	
	
	
	
	}	
	
}
