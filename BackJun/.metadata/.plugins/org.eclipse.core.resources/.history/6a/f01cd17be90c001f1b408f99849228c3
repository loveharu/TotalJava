package tjoen;

import java.time.DayOfWeek;
import java.time.LocalDateTime;
import java.util.*;
import java.util.Scanner;

public class Day3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
		
		int sum = 0;
		while(sum<100) {
			int num = sc.nextInt();
			sum += num;
			if(sum>=100) {
                System.out.println(sum);
                break;
            }
		}

	}

}
