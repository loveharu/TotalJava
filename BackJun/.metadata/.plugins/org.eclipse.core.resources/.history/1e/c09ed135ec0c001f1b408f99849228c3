package tjoen;

import java.time.DayOfWeek;
import java.time.LocalDateTime;
import java.util.*;
import java.util.Scanner;

public class Day3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		for(int i = 1; i <= 100; i++) {
			System.out.println(i + "     \t");
			if(i%num == 0) {
				System.out.println();
			}
			
		}

	}

}
