package tjoen;

import java.time.DayOfWeek;
import java.time.LocalDateTime;
import java.util.*;
import java.util.Scanner;

public class Day3 {

	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int count = 3;
	int sum = 0;
	while(count <= 35) {
		sum += count;
        count++;
	}
	System.out.println(sum);
	}

}
