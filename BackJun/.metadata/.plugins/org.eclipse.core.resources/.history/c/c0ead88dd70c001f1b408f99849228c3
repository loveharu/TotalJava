package tjoen;

import java.time.DayOfWeek;
import java.time.LocalDateTime;
import java.util.*;
import java.util.Scanner;

public class Day3 {

	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int num = sc.nextInt();
	for(int i = 0; i <= num; i+=2) {
		System.out.println(i);
	}
	}

}
